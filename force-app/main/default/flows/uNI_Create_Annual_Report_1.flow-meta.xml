    <description>Annual Report that supports versioning and reprogramming logics.</description>
    <environments>Default</environments>
    <formulas>
        <name>ARDueDate</name>
        <dataType>Date</dataType>
        <expression>DATE({!SelectedReportYearNumber},12,31)</expression>
    </formulas>
    <formulas>
        <name>ARPeriodEnd</name>
        <dataType>Date</dataType>
        <expression>DATE({!SelectedReportYearNumber},12,31)</expression>
    </formulas>
    <formulas>
        <name>ARPeriodStart</name>
        <dataType>Date</dataType>
        <expression>DATE({!SelectedReportYearNumber},1,1)</expression>
    </formulas>
    <formulas>
        <description>Cutoff date for reprogramming eligibility (defaults to June 1 of the selected report year).</description>
        <name>ARCutoffDate</name>
        <dataType>Date</dataType>
        <expression>DATE({!SelectedReportYearNumber},6,1)</expression>
    </formulas>
    <formulas>
        <name>ARYEAR</name>
        <dataType>Number</dataType>
        <expression>{!SelectedReportYearNumber}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>ARYearTextfromIA</name>
        <dataType>String</dataType>
        <expression>TEXT({!SelectedReportYearNumber})</expression>
    </formulas>
    <formulas>
        <name>SelectedLogframeVersionForAR</name>
        <dataType>String</dataType>
        <expression>IF({!get_Reprogramming_requests.logframeVersion}=&quot;0&quot;,{!get_Individual_Application.uNI_LogframeVersion__c},{!get_Reprogramming_requests.logframeVersion})</expression>
    </formulas>
    <formulas>
        <name>CurrentYear</name>
        <dataType>Number</dataType>
        <expression>YEAR({!$Flow.CurrentDate})</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>EndYear</name>
        <dataType>Number</dataType>
        <expression>YEAR({!get_Individual_Application.uNI_ImplementationEndDate__c})</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>FirstSelectableYear</name>
        <dataType>Number</dataType>
        <expression>IF({!StartYear} &gt; {!CurrentYear},{!StartYear},{!CurrentYear})</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>SelectedReportNumber</name>
        <dataType>Number</dataType>
        <expression>({!SelectedReportYearNumber} - {!StartYear}) + 1</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>SelectedReportYearNumber</name>
        <dataType>Number</dataType>
        <expression>VALUE({!annualreportchoice.selectedYear})</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>StartYear</name>
        <dataType>Number</dataType>
        <expression>YEAR({!get_Individual_Application.uNI_ImplementationStartDate__c})</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>v_CuttoffDate</name>
        <dataType>Date</dataType>
        <expression>DATE({!v_ReportCalenderYear},7,1)</expression>
    </formulas>
    <formulas>
        <description>Variable used for annual report Year computing.</description>
        <name>VarYearCounter</name>
        <dataType>String</dataType>
        <expression>&apos;Year_&apos;&amp;TEXT({!SelectedReportNumber})</expression>
    </formulas>
