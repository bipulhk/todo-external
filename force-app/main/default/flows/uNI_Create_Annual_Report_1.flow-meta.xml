    <decisions>
        <name>is_selected_year_in_range</name>
        <label>is selected year in range</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Invalid_Selected_Year</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Invalid Year</defaultConnectorLabel>
        <rules>
            <name>Selected_Year_Valid</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>SelectedReportYearNumber</leftValueReference>
                <operator>GreaterThanOrEqualTo</operator>
                <rightValue>
                    <elementReference>FirstSelectableYear</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>SelectedReportYearNumber</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <elementReference>EndYear</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Annual_Report_of_current_year</targetReference>
            </connector>
            <label>Selected Year Valid</label>
        </rules>
